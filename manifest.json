{
    "name": "MyChromeExtension",
    "manifest_version": 2,
    "version": "1.0.0",
    "description": "MyChromeExtension - Chrome 浏览器插件 By MoonLord",
    "author": "me@moonlord.cn",
    "browser_action": {
      "default_icon": "img/icon.png",
      "default_title": "MyChromeExtension - Chrome 浏览器插件 By MoonLord"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
          "lib/grayscale.js",
          "all.js"
        ],
        "all_frames": true,
        "run_at": "document_end"
      },
      {
        "matches": [
            "http://login.huawei.com/*", "https://login.huawei.com/*"
        ],
        "js": ["login.huawei.com.js"],
        "all_frames": true,
        "run_at": "document_end"
      },
      {
        "matches": [
            "http://w3.huawei.com/*","https://w3.huawei.com/*"
        ],
        "js": ["w3.huawei.com.js"],
        "all_frames": true,
        "run_at": "document_end"
      },
      {
        "matches": [
            "http://xinsheng.huawei.com/*", "https://xinsheng.huawei.com/*"
        ],
        "js": ["xinsheng.huawei.com.js"],
        "all_frames": true,
        "run_at": "document_end"
      }
    ]
}